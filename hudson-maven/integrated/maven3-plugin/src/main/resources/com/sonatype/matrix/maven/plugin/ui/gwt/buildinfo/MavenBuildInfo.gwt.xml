<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="MavenBuildInfo">
    <inherits name="com.sonatype.matrix.gwt.common.Common"/>
    <inherits name="com.sonatype.matrix.maven.model.Model"/>
    <inherits name="com.google.common.collect.Collect"/>
    <inherits name="com.sonatype.matrix.gwt.theme.chrome.Chrome"/>
    
    <!-- "" is the current directory. -->
    <source path="">
        <!--
            Exclude POJO test classes that Eclipse GWT Dev mode and the  GWTJUnit runner find.
        -->
        <exclude name="**/*Test.java"/>
    </source>
    
    <entry-point class="com.sonatype.matrix.maven.plugin.ui.gwt.buildinfo.MavenBuildInfoEntryPoint"/>
</module>