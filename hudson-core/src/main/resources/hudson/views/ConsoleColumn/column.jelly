<j:jelly xmlns:j="jelly:core">
  <td>
    <j:choose>
      <j:when test="${job.lastBuild!=null}">
        <a href="${jobBaseUrl}${job.shortUrl}lastBuild/console">
          <img src="${imagesURL}/${subIconSize}/terminal.gif" alt="${%Console output}"
               border="0"/>
        </a>
      </j:when>
      <j:otherwise>
        ${%N/A}
      </j:otherwise>
    </j:choose>
  </td>

</j:jelly>
